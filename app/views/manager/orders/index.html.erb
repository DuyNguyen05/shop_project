<div class="card">
  <div class="card-body">
    <h4 class="card-title"><b>Order Management</b></h4>
    <br>
    <div class="table-responsive">
      <div id="zero_config_wrapper" class="dataTables_wrapper container-fluid dt-bootstrap4">
        <div class="row">
          <div class="col-sm-12">
            <table id="table-orders" class="table table-striped table-bordered dataTable" role="grid" aria-describedby="zero_config_info">
              <thead>
                <tr role="row">
                  <th class="sorting_asc" tabindex="0" aria-controls="zero_config" rowspan="1" colspan="1" aria-sort="ascending" aria-label="Name: activate to sort column descending" style="width: 40px;">
                    ID
                  </th>
                  <th class="sorting_asc" tabindex="0" aria-controls="zero_config" rowspan="1" colspan="1" aria-sort="ascending" aria-label="Name: activate to sort column descending" style="width: 80px;">
                    Name
                  </th>
                  <th class="sorting" tabindex="0" aria-controls="zero_config" rowspan="1" colspan="1" aria-label="Position: activate to sort column ascending" style="width: 160px;">
                    Phone
                  </th>
                  <th class="sorting" tabindex="0" aria-controls="zero_config" rowspan="1" colspan="1" aria-label="Position: activate to sort column ascending" style="width: 160px;">
                    Address
                  </th>
                  <th class="sorting" tabindex="0" aria-controls="zero_config" rowspan="1" colspan="1" aria-label="Position: activate to sort column ascending" style="width: 120px;">
                      Status
                  </th>
                  <th class="sorting" tabindex="0" aria-controls="zero_config" rowspan="1" colspan="1" aria-label="Office: activate to sort column ascending" style="width: 40px;">
                    Time
                  </th>
                  <th style="width: 30px;"></th>
                </tr>
              </thead>
              <tbody id="table_order">
                <%= render "manager/orders/order", orders: @orders %>
              </tbody>
              <tfoot>
                <tr>
                  <th rowspan="1" colspan="1">
                    ID
                  </th>
                  <th rowspan="1" colspan="1">
                    User Name
                  </th>
                  <th rowspan="1" colspan="1">
                    Phone
                  </th>
                  <th rowspan="1" colspan="1">
                    Address
                  </th>
                  <th rowspan="1" colspan="1">
                    Status
                  </th>
                  <th rowspan="1" colspan="1">
                    Time
                  </th>
                  <th rowspan="1" colspan="1"></th>
                </tr>
              </tfoot>
            </table>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
<div id="myModal2"></div>
